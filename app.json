{
  "expo": {
    "name": "fasola",
    "slug": "fasola",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fasola",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.erykpiast.fasola",
      "buildConfiguration": "Debug",
      "infoPlist": {
        "UIFileSharingEnabled": true,
        "LSSupportsOpeningDocumentsInPlace": true
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icon/bold.svg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#7B9E30",
          "dark": {
            "backgroundColor": "#7B9E30"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "17.0"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "783aa343-f1f6-4fda-9425-2f305a8aca3a"
      }
    },
    "owner": "erykpiast",
    "android": {
      "package": "com.erykpiast.fasola"
    }
  }
}
